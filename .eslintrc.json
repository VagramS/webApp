{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "implicit-arrow-linebreak": 0,
      "camelcase": 0,
      "linebreak-style": 0,
      "no-console": 0,
      "nonblock-statement-body-position": 0,
      "curly": 0,
      "no-unused-vars": 0
    }
}
